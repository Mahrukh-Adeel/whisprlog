{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "allowImportingTsExtensions": false,
    "forceConsistentCasingInFileNames": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
